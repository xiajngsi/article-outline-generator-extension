{
    "name": "Better Outline",
    "description": "Auto generate outline for readme, article, more",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "scripting"
    ],
    "content_scripts": [
        {
            "css": [
                "style.css"
            ],
            "js": [
                "index.js"
            ],
            "matches": [
                "*://*/*"
            ],
            "run_at": "document_end"
        }
    ]
}